{

"name": "dashboard",
"description": "Replaces new tab",
"version": "1.0.0",

"manifest_version": 2,

"content_scripts": [{
      "matches": ["<all_urls>"],
        "js": [
            "popup.js"
              ]
}],

"browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
},

"chrome_url_overrides": {
      "newtab": "index.html"
},

"permissions":[
   "tabs",
   "storage"
]

}